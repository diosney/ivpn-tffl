{
  "name": "tffl-angular",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "npm start -- --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer ./dist/admin-spa/stats-es2015.json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en-US,es-ES}.json --string-as-default-value --clean --sort --format json --marker _t --format-indentation ' '",
    "i18n:translate": "node tools/i18n-translate",
    "i18n:all": "npm run i18n:extract && npm run i18n:translate",
    "deploy-to-s3": "aws s3 cp dist/tffl-angular s3://csr-tool-website/ --recursive --acl public-read && aws cloudfront create-invalidation --distribution-id ETVLH9T0ZHGWO  --paths '/*' && aws cloudfront create-invalidation --distribution-id ETVLH9T0ZHGWO  --paths '/*' ",
    "deploy-to-s3-dev": "aws s3 cp dist/tffl-angular s3://csr-tool-website-dev/ --recursive --acl public-read && aws cloudfront create-invalidation --distribution-id EAJDE5BOR9307  --paths '/*' && aws cloudfront create-invalidation --distribution-id EAJDE5BOR9307  --paths '/*' "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@aws-amplify/auth": "^4.6.13",
    "@cds/core": "^6.13.0",
    "@clr/angular": "^17.2.1",
    "@clr/ui": "^17.2.1",
    "@iplab/ngx-color-picker": "^17.2.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@nicky-lenaers/ngx-scroll-to": "^14.0.0",
    "angular-code-input": "^2.0.0",
    "angular-imask": "^6.4.3",
    "animatewithsass": "^3.2.1",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "crypto-js": "^4.2.0",
    "decimal.js": "^10.4.3",
    "html-to-pdfmake": "^2.5.12",
    "jquery": "^3.7.1",
    "jsbarcode": "^3.11.6",
