    "@types/lodash.merge": "^4.6.9",
    "@types/lodash.mergewith": "^4.6.9",
    "@types/lodash.values": "^4.3.9",
    "@types/pdfmake": "^0.2.9",
    "jasmine-core": "^5.2.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
